generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../database.db"
}

model Usuario {
  id String @id @default(uuid())
  nome String
  email String
  senha String
  created_at DateTime? @default(now())
  updated_at DateTime? @default(now())

  @@map("usuario")
}
